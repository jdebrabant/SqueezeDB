	Jstor_Formatteddate = {Jun., 2002},
	Keywords = {distinct values, block sampling},
	Language = {English},
	Number = {2},
	Pages = {389--404},
	Publisher = {American Statistical Association, Institute of Mathematical Statistics, and Interface Foundation of America},
	Title = {A Block Sampling Approach to Distinct Value Estimation},
	Url = {http://www.jstor.org/stable/1391058},
	Volume = {11},
	Year = {2002},
	Bdsk-File-1 = {},
